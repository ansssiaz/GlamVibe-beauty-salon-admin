<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/new_appointment_navigation"
    app:startDestination="@id/bottomMenuFragment">

    <fragment
        android:id="@+id/bottomMenuFragment"
        android:name="com.glamvibe.glamvibeadmin.presentation.fragments.BottomMenuFragment"
        android:label="fragment_bottom_menu"
        tools:layout="@layout/fragment_bottom_menu" >
        <action
            android:id="@+id/action_bottomMenuFragment_to_newAppointmentFragment"
            app:destination="@id/newAppointmentFragment" />
        <action
            android:id="@+id/action_bottomMenuFragment_to_profileDataFragment"
            app:destination="@id/profileDataFragment" />
        <action
            android:id="@+id/action_bottomMenuFragment_to_authorizationFragment"
            app:destination="@id/authorizationFragment" />
        <action
            android:id="@+id/action_bottomMenuFragment_to_servicesCatalogFragment"
            app:destination="@id/servicesCatalogFragment" />
        <action
            android:id="@+id/action_bottomMenuFragment_to_mastersCatalogFragment"
            app:destination="@id/mastersCatalogFragment" />
        <action
            android:id="@+id/action_bottomMenuFragment_to_clientsFragment"
            app:destination="@id/clientsFragment" />
    </fragment>
    <fragment
        android:id="@+id/newAppointmentFragment"
        android:name="com.glamvibe.glamvibeadmin.presentation.fragments.NewAppointmentFragment"
        android:label="NewAppointmentFragment"
        tools:layout="@layout/fragment_new_appointment"/>
    <fragment
        android:id="@+id/promotionFragment"
        android:name="com.glamvibe.glamvibeadmin.presentation.fragments.PromotionFragment"
        android:label="PromotionFragment"
        tools:layout="@layout/fragment_promotion"/>
    <fragment
        android:id="@+id/serviceInformationFragment"
        android:name="com.glamvibe.glamvibeadmin.presentation.fragments.ServiceInformationFragment"
        android:label="ServiceInformationFragment"
        tools:layout="@layout/fragment_service_information"/>
    <fragment
        android:id="@+id/profileDataFragment"
        android:name="com.glamvibe.glamvibeadmin.presentation.fragments.ProfileDataFragment"
        android:label="FormFragment"
        tools:layout="@layout/fragment_profile_data"/>
    <fragment
        android:id="@+id/authorizationFragment"
        android:name="com.glamvibe.glamvibeadmin.presentation.fragments.AuthorizationFragment"
        android:label="AuthorizationFragment"
        tools:layout="@layout/fragment_authorization">
        <action
            android:id="@+id/action_authorizationFragment_to_registrationFragment"
            app:destination="@id/registrationFragment" />
        <action
            android:id="@+id/action_authorizationFragment_to_bottomMenuFragment"
            app:destination="@id/bottomMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/registrationFragment"
        android:name="com.glamvibe.glamvibeadmin.presentation.fragments.RegistrationFragment"
        android:label="RegistrationFragment"
        tools:layout="@layout/fragment_registration">
    </fragment>
    <fragment
        android:id="@+id/clientAppointmentsFragment"
        android:name="com.glamvibe.glamvibeadmin.presentation.fragments.ClientAppointmentsFragment"
        android:label="ClientAppointmentsFragment"
        tools:layout="@layout/fragment_client_appointments"/>
    <fragment
        android:id="@+id/servicesCatalogFragment"
        android:name="com.glamvibe.glamvibeadmin.presentation.fragments.ServicesCatalogFragment"
        android:label="ServicesCatalogFragment"
        tools:layout="@layout/fragment_catalog_services">
        <action
            android:id="@+id/action_servicesCatalogFragment_to_serviceInformationFragment"
            app:destination="@id/serviceInformationFragment" />
        <action
            android:id="@+id/action_servicesCatalogFragment_to_newServiceFragment"
            app:destination="@id/newServiceFragment" />
    </fragment>
    <fragment
        android:id="@+id/mastersCatalogFragment"
        android:name="com.glamvibe.glamvibeadmin.presentation.fragments.MastersCatalogFragment"
        android:label="MastersCatalogFragment"
        tools:layout="@layout/fragment_catalog_masters">
        <action
            android:id="@+id/action_mastersCatalogFragment_to_masterInformationFragment"
            app:destination="@id/masterInformationFragment" />
        <action
            android:id="@+id/action_mastersCatalogFragment_to_newMasterFragment"
            app:destination="@id/newMasterFragment" />
    </fragment>
    <fragment
        android:id="@+id/newServiceFragment"
        android:name="com.glamvibe.glamvibeadmin.presentation.fragments.NewServiceFragment"
        android:label="NewServiceFragment"
        tools:layout="@layout/fragment_new_service"/>
    <fragment
        android:id="@+id/masterInformationFragment"
        android:name="com.glamvibe.glamvibeadmin.presentation.fragments.MasterInformationFragment"
        android:label="MasterInformationFragment"
        tools:layout="@layout/fragment_master_information"/>
    <fragment
        android:id="@+id/newMasterFragment"
        android:name="com.glamvibe.glamvibeadmin.presentation.fragments.NewMasterFragment"
        android:label="NewMasterFragment"
        tools:layout="@layout/fragment_new_master"/>
    <fragment
        android:id="@+id/clientsFragment"
        android:name="com.glamvibe.glamvibeadmin.presentation.fragments.ClientsFragment"
        tools:layout="@layout/fragment_clients"
        android:label="ClientsFragment" >
        <action
            android:id="@+id/action_clientsFragment_to_clientAppointmentsFragment"
            app:destination="@id/clientAppointmentsFragment" />
    </fragment>
</navigation>